{
  "name": "formel",
  "version": "1.0.7",
  "description": "Simply and lightweight Js package for the browser and for Node.js that provides extra String methods",
  "main": "build/index.js",
  "scripts": {
    "examples": "rimraf -rf dist && parcel examples/index.html",
    "clean": "rimraf -rf build/index.js",
    "build": "npm run clean && rollup -c",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "jest": "^24.9.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.20.0",
    "rollup-plugin-babel": "^4.3.3"
  },
  "keywords": [
    "capitalize",
    "reverse string",
    "strip html",
    "format",
    "string",
    "utility",
    "umd"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haikelfazzani/formel.git"
  },
  "bugs": {
    "url": "https://github.com/haikelfazzani/formel.git/issues"
  },
  "homepage": "https://github.com/haikelfazzani/formel.git#readme",
  "author": "Haikel Fazzani",
