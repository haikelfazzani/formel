{
  "name": "split-views",
  "version": "2.1.0",
  "description": "Fast and Lightweight Utility for Resizable Split Views",
  "module": "build/index.esm.js",
  "main": "build/index.js",
  "scripts": {
    "rm:win": "rmdir /S /Q build",
    "rm:linux": "rmdir dirname build",
    "dev": "sv --inject true --port 8001",
    "watch": "set NODE_ENV=developement && rollup -w -c",
    "build": "yarn rm:win &&set NODE_ENV=production && rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wutility/split-views.git"
  },
  "bugs": {
    "url": "https://github.com/wutility/split-views.git/issues"
  },
  "homepage": "https://github.com/wutility/split-views.git#readme",
  "author": "Haikel Fazzani",
  "license": "MIT",
  "devDependencies": {
    "rollup": "^2.35.1",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-terser": "^7.0.2",
    "servino": "0.0.6"
  },
  "keywords": [
    "resizer",
    "pane",
    "split",
    "view",
    "utility"
  ]
}